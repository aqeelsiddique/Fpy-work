<div class=" text-center">
    <h1>{{title}}</h1>
    {{!-- <h1>Welcome to VisioSpark Dashboard</h1><br><br> --}}
    <br><br>
    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-12">
                </div>
            </div>
            <div class="row">
                <div class="col-xl-3 col-md-6">
                    <div class="card mini-stat cart">
                        <div class="card-body mini-stat-img">
                            <div class="mini-stat-icon">
                                <i class="fa fa-users" style="font-size:48px;color:white"></i>
                                {{!-- <i class="mdi mdi-cube-outline float-right"></i> --}}
                            </div><br>
                            <div class="text-white">
                                <h6 class="text-uppercase mb-3">Total Teams</h6>
                                <h4 class="mb-4">{{data.team_count}}</h4>
                                {{!-- <span class="badge badge-info"> +11% </span> <span class="ml-2">From previous
                                    period</span> --}}
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-3 col-md-6">
                    <div class="card mini-stat cart">
                        <div class="card-body mini-stat-img">
                            <div class="mini-stat-icon">
                                <i class="fa fa-book" style="font-size:48px;color: white"></i>

                                {{!-- <img src="/images/subject.png" alt=""> --}}

                                {{!-- <i class="mdi mdi-buffer float-right"></i> --}}
                            </div><br>
                            <div class="text-white">
                                <h6 class="text-uppercase mb-3">Total Subbjects</h6>
                                <h4 class="mb-4">{{data.subject_count}}</h4>
                                {{!-- <span class="badge badge-danger"> -29% </span> <span class="ml-2">From previous
                                    period</span> --}}
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-3 col-md-6">
                    <div class="card mini-stat cart">
                        <div class="card-body mini-stat-img">
                            <div class="mini-stat-icon">
                                <i class="fa fa-question-circle" style="font-size:48px;color:white"></i>

                                {{!-- <i class="mdi mdi-tag-text-outline float-right"></i> --}}
                            </div><br>
                            <div class="text-white">
                                <h6 class="text-uppercase mb-3">Total Questons</h6>
                                <h4 class="mb-4">{{data.question_count}}</h4>
                                {{!-- <span class="badge badge-warning"> 0% </span> <span class="ml-2">From previous
                                    period</span> --}}
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-3 col-md-6">
                    <div class="card mini-stat cart">
                        <div class="card-body mini-stat-img">
                            <div class="mini-stat-icon">
                                <i class="fa fa-trophy" style="font-size:48px;color:white"></i>

                                {{!-- <i class="mdi mdi-briefcase-check float-right"></i> --}}
                            </div><br>
                            <div class="text-white">
                                <h6 class="text-uppercase mb-3">All Winners</h6>
                                <h4 class="mb-4">5</h4>
                                {{!-- <span class="badge badge-info"> +89% </span> <span class="ml-2">From previous
                                    period</span> --}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div> <!-- container-fluid -->
    </div>
</div>
</br></br>
<div class="container-xl">
    <div class="table-responsive">
        <div class="table-wrapper">
            <div class="table-title">
                <div class="row">
                    <div class="col-sm-6">
                        <h2>Manage <b>EventHead</b></h2>
                    </div>
                    <div class="col-sm-6">
                        <form method="post" action="/evenhead">
                            <button class="btn btn-success" data-toggle="modal">
                                Add New TeamHead-Name
                                <i class="material-icons">&#xE147;</i>
                            </button>

                            {{!-- <a href="/add_Question" class="btn btn-success" data-toggle="modal"><i
                                    class="material-icons">&#xE147;</i> <span>Add New EventHead</span></a> --}}
                            <a href="/deltedata/{{this._id}}" class="btn btn-danger" data-toggle="modal"><i
                                    class="material-icons">&#xE15C;</i> <span>Delete</span></a>
                    </div>
                </div>
            </div>
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>
                            <span class="custom-checkbox">
                                <input type="checkbox" id="selectAll">
                                <label for="selectAll"></label>
                            </span>
                        </th>
                        <th>images</th>

                        <th></th>
                        <th></th>
                        <th>Name</th>
                        <th></th>
                        <th>Email_Adress</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    {{#each list_EventHead}}
                    <tr>
                        <td>
                            <span class="custom-checkbox">
                                <input type="checkbox" id="checkbox1" name="options[]" value="1">
                                <label for="checkbox1"></label>
                            </span>
                        </td>
                        <td><img src="/images/{{this.image}}" alt="A cute cat" /></td>
 
                        <td></td>
                        <td></td>
                        <td>{{this.name}}</td>
                        <td> </td>
                        <td>{{this.email}}</td>



                        <td> </td>
                        <td class="liststyle">

                            <form method="POST" action="/update_Question/{{_id}}">
                                <button class="btn" onclick="return confirm('Are you sure to update this record ?');"><i
                                        class="fa fa-edit"></i></button>

                            </form>
                            <form method="POST" action="/eventhead_team/{{_id}}">
                                <button class="btn" onclick="return confirm('Are you sure to delete this record ?');"><i
                                class="fa fa-trash"></i></button>

                            </form>
                        </td>
                        {{!-- <td>
                            <a href="/update_Question/{{this._id}}" class="edit" data-toggle="modal"><i
                                    class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a>
                            <a href="/delete_Question/{{this._id}}" class="delete" data-toggle="modal"><i
                                    class="material-icons" data-toggle="tooltip" title="Delete">&#xE872;</i></a>
                        </td>
                    </tr> --}}

                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>
<script src="public/plugins/raphael/raphael-min.js"></script>