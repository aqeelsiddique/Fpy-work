<div class="container-xl">
	<div class="table-responsive">
		<div class="table-wrapper">
			<div class="table-title">
				<div class="row">
					<div class="col-sm-6">
						<h2>Manage <b>QuestionName</b></h2>
					</div>
					<div class="col-sm-6">
            
						<a href="/add_Question" class="btn btn-success" data-toggle="modal"><i class="material-icons">&#xE147;</i> <span>Add New Subject</span></a>
						<a href="/deltedata/{{this._id}}" class="btn btn-danger" data-toggle="modal"><i class="material-icons">&#xE15C;</i> <span>Delete</span></a>						
					</div>
				</div>
			</div>
			<table class="table table-striped table-hover">
				<thead>
					<tr>
						<th>
							<span class="custom-checkbox">
								<input type="checkbox" id="selectAll">
								<label for="selectAll"></label>
							</span>
						</th>
					<th>ID</th>
					<th></th>
					<th></th>
					<th></th>
					<th></th>
					<th></th>
				<th>Question</th> 
					</tr>
				</thead>
				<tbody>
					    {{#each list_question}}
					<tr>
						<td>
							<span class="custom-checkbox">
								<input type="checkbox" id="checkbox1" name="options[]" value="1">
								<label for="checkbox1"></label>
							</span>
						</td>
						<td> {{this._id}}</td>
						<td></td>
						<td></td>
						<td></td>
						<td></td>
						<td></td>
						<td>{{this.ques}}</td>
						<td>
							<a href="/update_Question/{{this._id}}" class="edit" data-toggle="modal"><i class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a>
							<a href="/delete_Question/{{this._id}}" class="delete" data-toggle="modal"><i class="material-icons" data-toggle="tooltip" title="Delete">&#xE872;</i></a>
						</td>
					</tr>
					    {{/each}}                 
				</tbody>
			</table>     
		</div>
	</div>        
</div>


