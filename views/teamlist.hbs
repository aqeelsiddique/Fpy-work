<div class="container-xl">
	<div class="table-responsive">
		<div class="table-wrapper">
			<div class="table-title">
				<div class="row">
					<div class="col-sm-6">
						<h2>Manage <b>Team-Name</b></h2>
					</div>
					<div class="col-sm-6">

						<a href="/Addteam" class="btn btn-success" data-toggle="modal"><i
								class="material-icons">&#xE147;</i> <span>Add New Team</span></a>
						<a href="/deltedata/{{this._id}}" class="btn btn-danger" data-toggle="modal"><i
								class="material-icons">&#xE15C;</i> <span>Delete</span></a>
					</div>
				</div>
			</div>
			<table class="table table-striped table-hover">
				<thead>
					<tr>
						<th>
							<span class="custom-checkbox">
								<input type="checkbox" id="selectAll">
								<label for="selectAll"></label>
							</span>
						</th>
						<th>ID</th>
						<th></th>
						<th></th>
						<th></th>
						<th></th>
						<th></th>
						<th>Team-Name</th>
					</tr>
				</thead>
				<tbody>
					{{#each list_Team}}
					<tr>
						<td>
							<span class="custom-checkbox">
								<input type="checkbox" id="checkbox1" name="options[]" value="1">
								<label for="checkbox1"></label>
							</span>
						</td>
						<td> {{this._id}}</td>
						<td></td>
						<td></td>
						<td></td>
						<td></td>
						<td></td>
						<td>{{this.teamname}}</td>
						<td class="liststyle">
							{{!-- <a href="/update/{{this._id}}" class="edit" data-toggle="modal"><i
									class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a> --}}
							<form method="POST" action="/update/{{_id}}">
								<button class="btn" onclick="return confirm('Are you sure to update this record ?');"><i
										class="class=fas fa-edit"></i></button>

							</form>
							<form method="POST" action="/delete_team/{{_id}}">
								<button class="btn" onclick="return confirm('Are you sure to delete this record ?');"><i
										class="fa fa-trash"></i></button>

							</form>

							{{!-- <a href="/deltedata/{{_id}}" class="delete" data-toggle="modal"><i
									class="material-icons" data-toggle="tooltip" title="Delete">&#xE872;</i></a> --}}
						</td>
						{{!-- <td>
							<a href="/update/{{this._id}}" class="edit" data-toggle="modal"><i class="material-icons"
									data-toggle="tooltip" title="Edit">&#xE254;</i></a>
							<a href="/delete_team/{{this._id}}" class="delete" data-toggle="modal"><i
									class="material-icons" data-toggle="tooltip" title="Delete">&#xE872;</i></a>
						</td> --}}
					</tr>
					{{/each}}
				</tbody>
			</table>
		</div>
	</div>
</div>